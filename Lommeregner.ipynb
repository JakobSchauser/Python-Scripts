    "import numpy as np\n",
    "energies = [-1,-1,-1,-2]\n",
    "k = 1.38e-23\n",
    "\n",
    "def Z(E,T):\n",
    "    exp = -1/(k*T)\n",
    "    s = sum([np.exp(e*exp) for e in E])\n",
    "    return(s)\n",
    "\n",
    "def P(e,E,T):\n",
    "    Z = Z(E,T)\n",
    "    return(np.e**(-e)*(E.count(e))/Z)\n"
